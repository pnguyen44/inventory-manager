{{inventory-list category=category editCategory='editCategory' deleteCategory='deleteCategory' createItem='createItem' deleteItem='deleteItem' editItem='editItem'}}
{{!-- {{#each model as |category|}}
  {{item-list category=category}} --}}
{{/each}}
